{
  "manifest_version": 2,
  "name": "Tinder Like Automático - Brasil [Jbrito]",
  "version": "1.0",
  "description": "Tinder Like Automático - Brasil, totalmente de graça. Sem nenhuma taxa futura!  [Jbrito] :)",

  "icons" : {
    "16" : "images/al-16.png",
    "48" : "images/al-48.png",
    "128" : "images/al-128.png"
  },
  "browser_action": {
    "default_icon" : "images/default-icon.png"
  },
  "content_scripts": [
    {
      "js": ["script.js"],
      "css": ["style.css"],
      "matches": ["https://tinder.com/*"]
    }
  ]
}
